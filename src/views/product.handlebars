<h1>Producto</h1>
<li class='nav-item'>
  <a class='nav-link' href='/products'>Volver al inicio</a>
</li>

  <div class="d-flex col col-xxl-6 col-xl-3 col-lg-6 m-3" style="width: 18rem; height:520px">
      <div class="card mb-0" style="width: 18rem; height:520px">
        <p class="mb-0">thumbnails:</p>
        <img class="card-img-top" style="width: 16rem; height: 150px" src="{{product.thumbnails}}" alt="">
        <div class="card-body" style="height: 300px; padding: 0px;">
          <h5 class="card-title mb-0">title: {{product.title}}</h5>
          <p class="card-text mb-0">description: {{product.description}}</p>
          <h5 class="card-title mb-0">code: {{product.code}}</h5>
          <h5 class="card-title mb-0">price: {{product.price}}</h5>
          <h5 class="card-title mb-0">status: {{product.status}}</h5>
          <h5 class="card-title mb-0">stock: {{product.stock}} </h5>
          <h5 class="card-title">category: {{product.category}}</h5>
          <button class="btn btn-success" onclick="putIntoCart('{{product._id}}')">Agregar al Carrito</button>
        </div>
      </div>
    </div>
  


<script src="/js/product.js"></script>